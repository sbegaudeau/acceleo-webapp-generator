<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.obeonetwork.pim.gen.bootstrap.model.bootstrap.presentation.BootstrapModelWizardID"
            name="%_UI_BootstrapModelWizard_label"
            class="org.obeonetwork.pim.gen.bootstrap.model.bootstrap.presentation.BootstrapModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/BootstrapModelFile.gif">
         <description>%_UI_BootstrapModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.obeonetwork.pim.gen.bootstrap.model.bootstrap.presentation.BootstrapEditorID"
            name="%_UI_BootstrapEditor_label"
            icon="icons/full/obj16/BootstrapModelFile.gif"
            extensions="bootstrap"
            class="org.obeonetwork.pim.gen.bootstrap.model.bootstrap.presentation.BootstrapEditor"
            contributorClass="org.obeonetwork.pim.gen.bootstrap.model.bootstrap.presentation.BootstrapActionBarContributor">
      </editor>
   </extension>

</plugin>
