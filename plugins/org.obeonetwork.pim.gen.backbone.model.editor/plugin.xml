<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.obeonetwork.pim.gen.backbone.model.backbone.presentation.BackboneModelWizardID"
            name="%_UI_BackboneModelWizard_label"
            class="org.obeonetwork.pim.gen.backbone.model.backbone.presentation.BackboneModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/BackboneModelFile.gif">
         <description>%_UI_BackboneModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.obeonetwork.pim.gen.backbone.model.backbone.presentation.BackboneEditorID"
            name="%_UI_BackboneEditor_label"
            icon="icons/full/obj16/BackboneModelFile.gif"
            extensions="backbone"
            class="org.obeonetwork.pim.gen.backbone.model.backbone.presentation.BackboneEditor"
            contributorClass="org.obeonetwork.pim.gen.backbone.model.backbone.presentation.BackboneActionBarContributor">
      </editor>
   </extension>

</plugin>
